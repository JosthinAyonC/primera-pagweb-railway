{{>head}}

{{>navbar}}


<div id="post" class="wrapper bg-img" data-bg="pic01.jpg">
	
		<div class="formulary">

		<h2>Dejame un mensaje de retroalimentacion!</h2>

		<form action="#" method="post">

		<div class="field half first">
			<label for="name">Name <span class="span">*</span></label>
			<input name="name" id="nameF" type="text" placeholder="Name">
		</div>
		<div class="field half">
			<label for="email">Email</label>
			<input name="email" id="emailF" type="email" placeholder="Email">
		</div>
		<div class="field">
			<label for="message">Message <span class="span">*</span> </label>
			<textarea name="message" id="messageF" rows="6" placeholder="Message"></textarea>
		</div>
		<ul class="actions">
			<li><input value="Send Message" class="button alt" type="button" onclick="capturar()"></li>
		</ul>
		</form>
		
	</div>
</div>


{{>footer}}